{
  "name": "clustr-workspace",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "author": "Makito <i@maki.to>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumimakito/clustr.git"
  },
  "scripts": {
    "build": "pnpm -r run build",
    "build:lib": "pnpm -F {./packages/lib} run build",
    "build:play": "pnpm -F {./packages/play}... run build",
    "dev": "pnpm -F {./packages/play} run dev",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache . --fix",
    "spellcheck": "cspell .",
    "test": "pnpm -r run test",
    "postinstall": "npx simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@tsconfig/strictest": "catalog:",
    "cspell": "catalog:",
    "eslint": "catalog:",
    "lint-staged": "catalog:",
    "simple-git-hooks": "catalog:",
    "typescript": "catalog:"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --cache"
    ]
  }
}
