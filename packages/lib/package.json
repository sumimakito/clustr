{
  "name": "clustr",
  "type": "module",
  "version": "1.0.1",
  "packageManager": "pnpm@10.12.4",
  "description": "Stream UTF-8 bytes and read grapheme clusters safely",
  "author": "Makito <i@maki.to>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumimakito/clustr.git"
  },
  "keywords": [
    "emoji",
    "grapheme",
    "grapheme cluster",
    "readable-stream",
    "stream",
    "streaming",
    "unicode",
    "utf8",
    "zwj-sequences"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=22.10.0"
  },
  "scripts": {
    "build": "tsdown",
    "bump": "bumpp --no-tag --no-push --sign",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache . --fix",
    "test": "vitest"
  },
  "devDependencies": {
    "@tsconfig/strictest": "catalog:",
    "@types/node": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
